// the global Array
var s = [23, 65, 98, 5];

//Adding a custom filter function to the prototype
Array.prototype.myFilter = function(callback){

//Create a new empty array
var newArray = [];

//Traverse through the array using the forEach method
s.forEach(function(item, index){
//If the callback parameter is true then push the current value into the new array
if(callback(item)) {
   newArray.push(item);
 };
})
//Return the new copy
  return newArray;
};

//Creates a variable from the s.myFilter function
var new_s = s.myFilter(function(item){
  return item % 2 === 1;
});
